{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --dotenv .env.local",
    "preview:staging": "nuxt preview --dotenv .env.dev",
    "preview:production": "nuxt preview --dotenv .env.prd",
    "build:staging": "nuxt build --dotenv .env.dev",
    "build:production": "nuxt build --dotenv .env.prd",
    "generate:staging": "nuxt generate --dotenv .env.dev",
    "generate:production": "nuxt generate --dotenv .env.prd",
    "generate-api": "orval --config ./src/api/orval.config.ts",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src/{components,composables,features,layouts,pages,plugins}/*.{vue,ts}",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "postinstall": "nuxt prepare && husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/lodash": "^4.17.0",
    "axios": "^1.6.8",
    "nuxt": "^3.11.1",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "yup": "^1.4.0",
    "yup-locale-ja": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@nuxt/eslint-config": "^0.3.1",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-access": "^2.1.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "msw": "^2.2.13",
    "orval": "^6.27.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "vite-plugin-stylelint": "^5.3.1",
    "vitest": "^1.4.0"
  },
  "msw": {
    "workerDirectory": "src/public"
  }
}
